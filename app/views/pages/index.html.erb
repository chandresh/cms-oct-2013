<table>
  <tr>
    <th>Name</th>
    <th>Title</th>
    <th>Actions</th>
  </tr>
  <% @pages.each do |page| %>
      <tr>
        <td><%= page.name %></td>
        <td><%= page.title %></td>
        <td><%= link_to "Edit", edit_page_path(page) %> | Delete</td>
      </tr>
  <% end %>
</table>

